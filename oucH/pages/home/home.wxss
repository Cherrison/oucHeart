.page {
  margin: 0rpx 50rpx 50rpx 50rpx;
}
.container {

    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    position: absolute;
}
.swiper {
    height: 300rpx;
}
.card-buttom{
  display: flex;
  flex-direction: row;
}
.swiper-image {
    width: 100%;
    height: 300rpx;
}

.play {
    top: -6px;
    width: 700rpx;
    margin: 0 auto;
    position: relative;
    border-radius: 10rpx;
    background-color: #fff;
    box-shadow: 0 2rpx 16rpx rgba(172,172,172,.5);
    padding: 28rpx 24rpx;
    box-sizing: border-box;
}

.play-main {
    overflow: hidden;
}

.play-main .photo {
    float: left;
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}

.play-box {
    margin-left: 118rpx;
    position: relative;
    padding: 10rpx 0;
    padding-left: 80rpx;
}

.play-box .key {
    left: 0;
    top: 16rpx;
    color: #fff;
    font-size: 24rpx;
    width: 68rpx;
    height: 28rpx;
    text-align: center;
    line-height: 28rpx;
    border-radius: 6rpx;
    background-color: #b9bd94;
    position: absolute;
}

.play-box .tit {
    font-size: 30rpx;
    height: 40rpx;
    line-height: 40rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-weight: bold;
} 

.play-box .text {
    font-size: 26rpx;
    color: #999;
    height: 28rpx;
    line-height: 28rpx;
    margin-top: 12rpx;
    white-space: nowrap;
}

.play-box .text label {
    display: inline-block;
    height: 26rpx;
    line-height: 26rpx;
    color: #2ec17d;
    font-size: 20rpx;
    padding: 0 7rpx;
    border-radius: 6rpx;
    border: 1rpx solid #2ec17d;
    margin-left: 10rpx;
}

.slider {
    height: 36rpx;
    line-height: 36rpx;
    padding: 16rpx;
}

.slider .left,.slider .right {
    font-size: 22rpx;
    color: rgb(35, 14, 94);
}

.slider .left {
    float: left;
    padding-left: 10rpx;
}

.slider .right {
    float: right;
    padding-right: 10rpx;
}

.slider-box {
    margin: 0 92rpx;
}

.play-bottom {
    line-height: 80rpx;
    text-align: center;
    padding-top: 12rpx;
    padding-bottom: 16rpx;
}

.play-bottom .play-prev,.play-bottom .play-next {
    display: inline-block;
    vertical-align: top;
}

.play-bottom .play-prev image,.play-bottom .play-next image {
    width: 39rpx;
    height: 44rpx;
    vertical-align: middle;
}

.play-bottom .play-btn {
    width: 80rpx;
    height: 80rpx;
    display: inline-block;
    vertical-align: top;
    border-radius: 50%;
    background-image: url('https://www.52hertalk.cn/public/static/images/wx/play-btn-start.png');
    background-position: center;
    background-size: 100%;
    box-shadow: 0 3rpx 24rpx rgba(46,193,125,.5);
    margin: 0 90rpx;
}

.play-bottom .play-btn.stop {
    background-image: url('https://www.52hertalk.cn/public/static/images/wx/play-btn-stop.png');
}

.topPlay {
    top: auto;
    bottom: 0;
    width: 100%;
    position: fixed;
    border-radius: 0;
    z-index: 9;
    padding: 8rpx 24rpx;
    box-shadow: 0 1rpx 1px #dcdcdc;
}

.topPlay .play-box {
    padding-left: 0;
    padding-right: 360rpx;
}

.topPlay .play-box .key {
    display: none;
}

.topPlay .play-box .text label {
    display: none;
}

.topPlay .slider {
    display: none;
}

.topPlay .play-bottom {
    top: 8rpx;
    right: 24rpx;
    position: absolute;
    padding: 10rpx 0;
}

.topPlay .play-bottom .play-btn {
    margin: 0 72rpx;
}

.menu {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    margin-left: 40rpx;
    margin-right: 40rpx;
}

.menu image{
  width: 100%;
  height: 100%;
}
.menu .item {
    float: left;
    text-align: center;
}

.menu .item-ic {
    width: 95rpx;
    height: 95rpx;
}

.menu .item-name {
    color: #333;
    font-size: 24rpx;
    margin-top: 10rpx;
    padding-bottom: 2rpx;
    height: 32rpx;
    line-height: 32rpx;
    font-weight: bold;
}

.book {
    width: 100%;
    height: 430rpx;
    position: relative;
    /* margin 是外边距 */
    margin-top: 20rpx;  
}

.book-item {
    width: 682rpx;
    height: 400rpx;
    position: relative;
    margin: 0 auto;
    transition-duration: 500ms;
    z-index: 1;
}

.book .book-item .pic {
    width: 100%;
    height: 400rpx;
    display: block;
    border-radius: 20rpx;
}

.book-item-bg-1 {
    z-index: 0;
    transform: translate3d(24rpx,16rpx,0);
}

.book-item-bg-2 {
    z-index: -1;
    transform: translate3d(44rpx,30rpx,0);
}

.book .item {
    margin: 0 auto;
    width: 682rpx;
    height: 400rpx;
    background-color: rgba(0,0,0,.2);
    border-radius: 20rpx;
    box-shadow: 0 2rpx 16rpx rgba(172,172,172,.5);
    box-sizing: border-box;
    padding: 0 48rpx;
    position: absolute;
    top: 0;
}

.book .item-main {
    padding: 18rpx 6rpx 6rpx;
}

.book .item-tit {
    font-size: 30rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin-bottom: 13rpx;
    font-weight: bold;
}

.book .item-text {
    height: 192rpx;
    color: #fff;
    font-size: 26rpx;
    line-height: 48rpx;
    overflow: hidden;
    white-space: normal;
    display: -webkit-box;
    -webkit-line-clamp: 4;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
}

.book .item-box {
    text-align: right;
}

.book .item-box text {
    height: 52rpx;
    line-height: 52rpx;
    border-radius: 15rpx;
    padding: 0 20rpx;
    font-size: 22rpx;
    color: #fff;
    vertical-align: top;
    display: inline-block;
}

.book .item-bottom {
    overflow: hidden;
    height: 60rpx;
    padding: 10rpx 0;
    border-top: 1rpx solid #dcdcdc;
}


.card-bottom .li {
    color: gold;
    width: 33.333%;
    display: block;
    float: left;
    text-align: center;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.book .item-bottom .li view {
    display: inline-block;
    font-size: 24rpx;
    color: #fff;
    height: 60rpx;
    line-height: 60rpx;
    padding-left: 50rpx;
    padding-right: 6rpx;
    background-repeat: no-repeat;
    background-position: left center;
    background-size: 44rpx;
}

.card-bottom .li .praise {
    color: gold
}


.card-bottom .li .praise.active {
    color: greenyellow
}


.home-tit {
    height: 64rpx;
    line-height: 64rpx;
    padding: 28rpx 24rpx;
}

.home-tit .name {
    float: left;
    font-size: 36rpx;
    font-weight: bold;
}

.home-tit .more {
    float: right;
    width: 38rpx;
    height: 38rpx;
    margin-top: 13rpx;
    background: url('https://www.52hertalk.cn/public/static/images/wx/ic-more.png');
    background-repeat: no-repeat;
    background-size: 100% auto;
}

.course {
    margin: 0 24rpx;
    border-radius: 10rpx;
    background-color: #fff;
    box-shadow: 0 2rpx 16rpx rgba(172,172,172,.5);
}

.course .item {
    display: block;
    padding: 24rpx;
    position: relative;
}

.course .item::before {
    content: "";
    bottom: -1rpx;
    left: 294rpx;
    width: 384rpx;
    height: 1px;
    display: block;
    position: absolute;
    background-color: #dcdcdc;
}

.course .item:last-child::before {
    display: none;
}

.course .item::after {
    content: "";
    display: block;
    clear: both;
}

.course .item .pic {
    width: 248rpx;
    height: 140rpx;
    float: left;
    border-radius: 10rpx;
}

.course .item-main {
    margin-left: 270rpx;
}

.course .item-main .tit {
    font-size: 28rpx;
    height: 30rpx;
    line-height: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-bottom: 8rpx;
}

.course .item-main .text {
    height: 64rpx;
    line-height: 32rpx;
    font-size: 22rpx;
    color: #999;
    margin-bottom: 8rpx;
    overflow: hidden;
    white-space: normal;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
}

.course .item-main .num {
    float: left;
    height: 30rpx;
    line-height: 30rpx;
    font-size: 22rpx;
    color: #2ec17d;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.course .item-main .num label {
    display: inline-block;
    border-radius: 4rpx;
    color: #fff;
    padding: 0 6rpx;
    margin-right: 10rpx;
    vertical-align: top;
    background-color: #2ec17d;
}

.course .item-main .box {
    overflow: hidden;
    height: 30rpx;
    line-height: 30rpx;
}

.course .item-main .price {
    float: left;
    font-size: 26rpx;
    color: #2ec17d;
    font-weight: bold;
    margin-left: 20rpx;
}

.course .item-main .price label {
    font-size: 20rpx;
    color: #999;
    text-decoration: line-through;
    font-weight: normal;
    margin-left: 20rpx;
}

.course .item-main .count {
    float: right;
    font-size: 22rpx;
    color: #999;
}

.article-swiper {
    height: 250rpx;
    white-space: nowrap;
    -webkit-overflow-scrolling: touch;
}

.article-swiper .item {
    width: 445rpx;
    height: 250rpx;
    display: inline-block;
    overflow: hidden;
    position: relative;
    border-radius: 15rpx;
    margin-left: 24rpx;
}

.article-swiper .item:last-child {
    margin-right: 24rpx;
}

.article-swiper .item image {
    width: 445rpx;
    height: 250rpx;
}

.article-swiper .item-main {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    padding: 16rpx;
    background-color: rgba(0,0,0,.2);
}

.article-swiper .item-main .tit {
    font-size: 32rpx;
    color: #fff;
    height: 40rpx;
    line-height: 40rpx;
    margin-bottom: 10rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.article-swiper .item-main .text {
    font-size: 22rpx;
    color: #fff;
    height: 114rpx;
    line-height: 38rpx;
    overflow: hidden;
    white-space: normal;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
}

.article-swiper .item-box {
    padding: 0 4px;
    overflow: hidden;
    margin-top: 16rpx;
}

.article-swiper .item-box view {
    height: 40rpx;
    line-height: 40rpx;
    padding-left: 48rpx;
    background-size: 34rpx;
    background-position: left;
    background-repeat: no-repeat;
    color: #fff;
}

.article-swiper .item-box .see {
    float: left;
    font-size: 22rpx;
    background-image: url('https://www.52hertalk.cn/public/static/images/wx/article-ic-eye.png');
}

.article-swiper .item-box .name {
    float: right;
    font-size: 24rpx;
    background-image: url('https://www.52hertalk.cn/public/static/images/wx/article-ic-user.png');
}

